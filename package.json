{
  "name": "li-daisy",
  "version": "0.6.0",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/li-daisy.umd.js",
  "module": "./dist/li-daisy.es.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/style.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/li-daisy.es.js",
      "require": "./dist/li-daisy.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "format": "prettier --write packages/ examples/",
    "type-check": "vue-tsc --noEmit",
    "build": "vite build",
    "lint": "eslint \"packages/**/*.{ts,vue,js}\" ",
    "lint:fix": "eslint \"packages/**/*.{ts,vue,js}\" --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm run build && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "daisyui": "^5.0.19",
    "pinia": "^3.0.2",
    "tailwindcss": "^4.1.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@tsconfig/node22": "^22.0.1",
    "@types/glob": "^8.1.0",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "glob": "^11.0.1",
    "jiti": "^2.4.2",
    "li-daisy": "workspace:*",
    "markdown-it-container": "^4.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "shiki": "^3.2.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.6",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.2.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lirous587/Li-Daisy.git"
  }
}
